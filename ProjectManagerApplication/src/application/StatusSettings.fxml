            </TableView>
            <TableView fx:id="tblTaskStatus" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
              <columns>
                <TableColumn prefWidth="402.0" text="Status" />
                <TableColumn prefWidth="130.0" text="Farbe" />
              </columns>
            </TableView>
         </children>
      </GridPane>
